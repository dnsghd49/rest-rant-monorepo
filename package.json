{
  "name": "rest-rant-monorepo",
  "version": "1.0.0",
  "description": "REST-Rant is an app where users can review restaurants.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd frontend && npm start",
    "server": "cd backend && npm start",
    "install": "concurrently \"npm run frontmod\" \"npm run backmod\"",
    "frontmod": "cd frontend && npm install",
    "backmod": "cd backend && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnsghd49/rest-rant-monorepo.git"
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  },
